datasource postgres {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  alias    String?
  id       String  @id @default(cuid())
  message  String?
  password String
  roles    Json
  username String  @unique
}

model User1 {
  alias   String?
  id      String  @id @default(cuid())
  message String?
}

model User2 {
  alias   String?
  id      String  @id @default(cuid())
  message String?
}
